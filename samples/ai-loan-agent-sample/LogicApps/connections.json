{
  "agentConnections": {
    "agent": {
      "displayName": "Azure OpenAI Agent",
      "endpoint": "@appsetting('agent_openAIEndpoint')",
      "resourceId": "@appsetting('agent_ResourceID')",
      "authentication": {
        "type": "ManagedServiceIdentity"
      },
      "type": "model"
    }
  },
  "managedApiConnections": {
    "formsConnection": {
      "api": {
        "id": "/subscriptions/@{appsetting('WORKFLOWS_SUBSCRIPTION_ID')}/providers/Microsoft.Web/locations/@{appsetting('WORKFLOWS_LOCATION_NAME')}/managedApis/microsoftforms"
      },
      "connection": {
        "id": "/subscriptions/@{appsetting('WORKFLOWS_SUBSCRIPTION_ID')}/resourceGroups/@{appsetting('WORKFLOWS_RESOURCE_GROUP_NAME')}/providers/Microsoft.Web/connections/formsConnection"
      },
      "connectionRuntimeUrl": "@appsetting('formsConnection-ConnectionRuntimeUrl')",
      "authentication": {
        "type": "ManagedServiceIdentity"
      }
    },
    "teamsConnection": {
      "api": {
        "id": "/subscriptions/@{appsetting('WORKFLOWS_SUBSCRIPTION_ID')}/providers/Microsoft.Web/locations/@{appsetting('WORKFLOWS_LOCATION_NAME')}/managedApis/teams"
      },
      "connection": {
        "id": "/subscriptions/@{appsetting('WORKFLOWS_SUBSCRIPTION_ID')}/resourceGroups/@{appsetting('WORKFLOWS_RESOURCE_GROUP_NAME')}/providers/Microsoft.Web/connections/teamsConnection"
      },
      "connectionRuntimeUrl": "@appsetting('teamsConnection-ConnectionRuntimeUrl')",
      "authentication": {
        "type": "ManagedServiceIdentity"
      }
    },
    "outlookConnection": {
      "api": {
        "id": "/subscriptions/@{appsetting('WORKFLOWS_SUBSCRIPTION_ID')}/providers/Microsoft.Web/locations/@{appsetting('WORKFLOWS_LOCATION_NAME')}/managedApis/office365"
      },
      "connection": {
        "id": "/subscriptions/@{appsetting('WORKFLOWS_SUBSCRIPTION_ID')}/resourceGroups/@{appsetting('WORKFLOWS_RESOURCE_GROUP_NAME')}/providers/Microsoft.Web/connections/outlookConnection"
      },
      "connectionRuntimeUrl": "@appsetting('outlookConnection-ConnectionRuntimeUrl')",
      "authentication": {
        "type": "ManagedServiceIdentity"
      }
    }
  },
  "serviceProviderConnections": {
    "azureblob": {
      "parameterValues": {
        "authProvider": "ManagedIdentity",
        "storageAccountName": "@appsetting('azureblob_storageAccountName')"
      },
      "serviceProvider": {
        "id": "/serviceProviders/AzureBlob"
      },
      "displayName": "Azure Blob Storage (Managed Identity)"
    },
    "sql": {
      "parameterValues": {
        "connectionString": "@appsetting('sql_connectionString')"
      },
      "serviceProvider": {
        "id": "/serviceProviders/sql"
      },
      "displayName": "sql"
    }
  },
  "apiManagementConnections": {
    "riskAssessmentAPI": {
      "apiId": "/subscriptions/@{appsetting('WORKFLOWS_SUBSCRIPTION_ID')}/resourceGroups/@{parameters('riskAssessmentAPI-ResourceGroup')}/providers/Microsoft.ApiManagement/service/@{parameters('riskAssessmentAPI-ServiceName')}/apis/olympia-risk-assessment",
      "baseUrl": "@parameters('riskAssessmentAPI-BaseUrl')",
      "subscriptionKey": "@appsetting('riskAssessmentAPI_SubscriptionKey')",
      "displayName": "Risk Assessment API"
    },
    "employmentValidationAPI": {
      "apiId": "/subscriptions/@{appsetting('WORKFLOWS_SUBSCRIPTION_ID')}/resourceGroups/@{parameters('employmentValidationAPI-ResourceGroup')}/providers/Microsoft.ApiManagement/service/@{parameters('employmentValidationAPI-ServiceName')}/apis/litware-employment-validation",
      "baseUrl": "@parameters('employmentValidationAPI-BaseUrl')",
      "subscriptionKey": "@appsetting('employmentValidationAPI_SubscriptionKey')",
      "displayName": "Employment Validation API"
    },
    "creditCheckAPI": {
      "apiId": "/subscriptions/@{appsetting('WORKFLOWS_SUBSCRIPTION_ID')}/resourceGroups/@{parameters('creditCheckAPI-ResourceGroup')}/providers/Microsoft.ApiManagement/service/@{parameters('creditCheckAPI-ServiceName')}/apis/cronus-credit",
      "baseUrl": "@parameters('creditCheckAPI-BaseUrl')",
      "subscriptionKey": "@appsetting('creditCheckAPI_SubscriptionKey')",
      "displayName": "Credit Check API"
    },
    "demographicVerificationAPI": {
      "apiId": "/subscriptions/@{appsetting('WORKFLOWS_SUBSCRIPTION_ID')}/resourceGroups/@{parameters('demographicVerificationAPI-ResourceGroup')}/providers/Microsoft.ApiManagement/service/@{parameters('demographicVerificationAPI-ServiceName')}/apis/northwind-demographic-verification",
      "baseUrl": "@parameters('demographicVerificationAPI-BaseUrl')",
      "subscriptionKey": "@appsetting('demographicVerificationAPI_SubscriptionKey')",
      "displayName": "Demographic Verification API"
    }
  }
}